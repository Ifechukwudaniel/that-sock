     function GetBackgroundColor(uint256 colorInt) public pure returns(string memory){ 
       if(colorInt == 0 ) {
            // WHITE
           return "#F5F5F5";
       }
       else if (colorInt == 1) {
            // GRAY
            return "#DCDCDC";
       }
       else if(colorInt == 2) {
          //Orange
          return"#FDB0B0";
       } 
       else if(colorInt == 3) {
         //Pink
         return "#FFD6F1";
       }
       else if(colorInt == 4) {
         //YELLOW
         return "#FCDFB4";
       }
       else if(colorInt == 5) {
         //BRIGHT YELLOW
         return "#FDFFBC";
       }
       else if(colorInt == 6) {
         //Green
         return "#A5F7C6";
       }
       else if(colorInt == 7) {
         //Blue
         return "#D6E4FF";
       }
       else if(colorInt == 8) {
         //Dark Blue
         return "#A5BCF7";
       }
       else if(colorInt == 9) {
         //Dark PinK
         return "#E8C2FF";
       }
       else {
         // Black
         return"#282828";
       }
     }

